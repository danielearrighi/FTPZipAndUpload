<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!--FOLDERS-->
    <add key="ZipOriginFolder" value="C:\Test\" />
    <add key="ZipDestinationFolder" value="C:\Temp\" />
    <add key="FTPFolder" value="Test/" />
    
    <!--OPTIONS-->
    <add key="ZipDestinationFileNameBase" value="Backup" />
    <add key="AppendDateTime" value="true" />
    <add key="DeleteFile" value="true" />
    <add key="CleanRemote" value="30" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />

    <!--CONNECTION INFO (Required)-->
    <add key="FTPServer" value="127.0.0.1" />
    <add key="FTPUser" value="username@something.net" />
    <add key="FTPPassword" value="" />
  </appSettings>
</configuration>